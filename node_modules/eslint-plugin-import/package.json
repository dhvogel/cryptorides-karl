    "eslint-module-utils": "^2.2.0",
    "has": "^1.0.1",
    "lodash": "^4.17.4",
    "minimatch": "^3.0.3",
    "read-pkg-up": "^2.0.0",
    "resolve": "^1.6.0"
  },
